<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.my.workoutguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WorkoutGuide">
        <activity android:name=".Loss"></activity>
        <activity android:name=".Gain" />
        <activity android:name=".ui.LEGA" />
        <activity android:name=".ui.CHESTA" />
        <activity android:name=".ui.BASA" />
        <activity android:name=".ui.ARMA" />
        <activity android:name=".ui.ABSA" />
        <activity android:name=".ui.BASI" />
        <activity android:name=".ui.LEGI" />
        <activity android:name=".ui.ARMI" />
        <activity android:name=".ui.CHESTI" />
        <activity android:name=".ui.ABSI" />
        <activity android:name=".ui.BASB" />
        <activity android:name=".ui.LEGB" />
        <activity android:name=".ui.ARMB" />
        <activity android:name=".ui.ChestB" />
        <activity android:name=".ui.ABSB" />
        <activity
            android:name=".NavigationDrawer"
            android:label="@string/title_activity_navigation_drawer"
            android:theme="@style/Theme.WorkoutGuide.NoActionBar" />
        <activity android:name=".MainActivity"
            android:label=""
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".LoadScreen"
            android:label=""
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:label=""
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <service android:name=".MyMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>

        </service>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>